{
  "name": "redstone-contracts-wasm",
  "version": "0.0.1",
  "description": "WASM contracts for SmartWeave protocol",
  "main": "index.js",
  "repository": "git@github.com:redstone-finance/redstone-contracts-wasm.git",
  "author": "ppedziwiatr <peter@redstone.finance>",
  "license": "MIT",
  "private": false,
  "devDependencies": {
    "@assemblyscript/loader": "^0.19.17",
    "assemblyscript": "^0.19.18",
    "json-as": "^0.2.6",
    "visitor-as": "^0.6.0"
  },
  "scripts": {
    "asbuild:untouched": "asc assembly/handle.ts --exportRuntime --enable reference-types --target debug",
    "asbuild:optimized": "asc assembly/handle.ts --exportRuntime --enable reference-types --target release",
    "asbuild": "yarn asbuild:untouched && yarn asbuild:optimized",
    "asbuild2:untouched": "asc assembly/RedStoneToken.ts --exportRuntime --sourceMap --importMemory --exportTable --target debug",
    "asbuild2:optimized": "asc assembly/RedStoneToken.ts --exportRuntime --sourceMap --importMemory --exportTable --target release",
    "asbuild2": "yarn asbuild2:untouched && yarn asbuild2:optimized",
    "test": "node tests"
  }
}
